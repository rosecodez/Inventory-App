extends layout

block content
  h1= title
  a.btn.btn-primary(href="/inventory-app/fan/create", role="button") Create new fan
  if fanList && fanList.length
    ul
      each fan in fanList
        li
          a(href=`/inventory-app/fan/${fan._id}`) #{fan.brand} #{fan.model} #{fan.type} #{fan.series} #{fan.fanCounts}
  else
    p No fans found
