extends layout

block content
  h1= title
  a.btn.btn-primary(href="/inventory-app/psu/create", role="button") Create new psu
  if psuList && psuList.length
    ul
      each psu in psuList
        li
          a(href=`/inventory-app/psu/${psu._id}`) #{psu.brand} #{psu.maxPower} #{psu.type} #{psu.series}
  else
    p No power supplies found
