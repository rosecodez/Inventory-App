extends layout

block content
  h1= title
  a.btn.btn-primary(href="/inventory-app/cpu/create", role="button") Create New CPU
  if cpuList && cpuList.length
    ul
      each cpu in cpuList
        li
          a(href=`/inventory-app/cpu/${cpu._id}`) #{cpu.brand} #{cpu.type} #{cpu.series} #{cpu.socket} #{cpu.model}
  else
    p No CPUs found
