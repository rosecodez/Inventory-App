extends layout

block content
  h1= title
  a.btn.btn-primary(href="/inventory-app/gpu/create", role="button") Create new graphics card
  if gpuList && gpuList.length
    ul
      each gpu in gpuList
        li
          a(href=`/inventory-app/gpu/${gpu._id}`) #{gpu.brand} #{gpu.GPU} #{gpu.series} #{gpu.gpuInterface} #{gpu.model}
  else
    p No GPUs found
